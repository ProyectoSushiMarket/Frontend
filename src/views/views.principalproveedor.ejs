    <!-- Modal para agregar producto -->
<div id="modal" class="modal">
    <div class="modal-content">
        <div class="modal-header">Agregar Producto</div>
        <div class="modal-body">
            <form id="product-form">
                <label for="product-name">Nombre del producto:</label>
                <input type="text" id="product-name" name="product-name" placeholder="Ingresa el nombre del producto">

                <label for="product-url">URL de la imagen:</label>
                <input type="text" id="product-url" name="product-url" placeholder="Ingresa la URL de la imagen">
            </form>
        </div>
        <div class="modal-actions">
            <button class="save-button" onclick="guardarCambios()">Guardar</button>
            <button class="cancel-button" onclick="cerrarModal()">Cancelar</button>
        </div>
    </div>
